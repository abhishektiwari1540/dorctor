{
  "name": "patientplanet",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "concurrently \"npm run dev:watch-prisma\" \"npm run start:dev\"",
    "dev:watch-prisma": "nodemon --watch prisma/models/ --ext prisma --exec \"npm run dev:merge\"",
    "dev:merge": "node merge-prisma.js",
    "dev:generate": "npx prisma generate",
    "dev:migrate": "npx prisma migrate dev --name dev-update",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "build": "nest build && npm run prod:merge && npm run prod:generate",
    "start": "node dist/main",
    "prod:merge": "node merge-prisma.js",
    "prod:generate": "npx prisma generate",
    "prod:migrate": "npx prisma migrate deploy",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js -d ./src/data-source.ts migration:run",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js -d ./src/data-source.ts migration:generate",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js -d ./src/data-source.ts migration:revert"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/platform-fastify": "^11.0.11",
    "@nestjs/typeorm": "^11.0.0",
    "@prisma/client": "^6.9.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.24"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.31",
    "@types/supertest": "^6.0.2",
    "concurrently": "^9.1.2",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "mysql2": "^3.14.1",
    "prettier": "^3.5.3",
    "prisma": "^6.9.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
